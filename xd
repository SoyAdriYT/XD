MinimizeButton.Parent = Frame
MinimizeButton.BackgroundColor3 = Color3.new(1, 0, 0)
MinimizeButton.BorderColor3 = Color3.new(0, 0, 0)
MinimizeButton.Position = UDim2.new(1, -30, 0, 0)
MinimizeButton.Size = UDim2.new(0, 30, 0, 30)
MinimizeButton.Font = Enum.Font.SourceSansBold
MinimizeButton.Text = "-"
MinimizeButton.TextColor3 = Color3.new(1, 1, 1)
MinimizeButton.TextSize = 16

local isMinimized = false
local originalSize = Frame.Size
local minimizedSize = UDim2.new(0, 100, 0, 30)

MinimizeButton.MouseButton1Click:Connect(function()
    isMinimized = not isMinimized
    if isMinimized then
        Frame:TweenSize(minimizedSize, Enum.EasingDirection.InOut, Enum.EasingStyle.Quart, 0.5, true)
    else
        Frame:TweenSize(originalSize, Enum.EasingDirection.InOut, Enum.EasingStyle.Quart, 0.5, true)
    end
end)

local function injectPitbullX()
    wait(14)
    game:GetService("StarterGui"):SetCore("SendNotification", {
    Title = "PitbullXüêï‚Äçü¶∫",
    Text = "PitbullX Se ha inyectado exitosamente en Roblox..",
    Icon = "", -- Puedes colocar aqu√≠ el asset ID de un √≠cono opcional
    Duration = 5 -- Duraci√≥n en segundos (opcional)
})
wait(2)
game:GetService("StarterGui"):SetCore("SendNotification", {
    Title = "PitbullX Anti-Cheat Bypass",
    Text = "PitbullX Ha eludido exitosamente el Anti-Trampas del juego.",
    Icon = "", -- Puedes colocar aqu√≠ el asset ID de un √≠cono opcional
    Duration = 5 -- Duraci√≥n en segundos (opcional)
})
    ExecuteButton.Visible = true
end

local function executeScript(script)
    game:GetService("StarterGui"):SetCore("SendNotification", {
    Title = "PitbullXüêï‚Äçü¶∫",
    Text = "PitbullX Ejecut√≥ un script exitosamente.",
    Icon = "", -- Puedes colocar aqu√≠ el asset ID de un √≠cono opcional
    Duration = 5 -- Duraci√≥n en segundos (opcional)
})
    loadstring(script)()
end

ExecuteButton.MouseButton1Click:Connect(function()
    local script = ScriptBox.Text
    if script ~= "" then
        executeScript(script)
    end
end)

DeleteButton.MouseButton1Click:Connect(function()
    ScriptBox.Text = ""
end)

InjectButton.MouseButton1Click:Connect(function()
    injectPitbullX()
end)

