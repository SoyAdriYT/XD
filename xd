ScriptBox.TextYAlignment = Enum.TextYAlignment.Top

ExecuteButton.Parent = Frame
ExecuteButton.BackgroundColor3 = Color3.new(0, 0, 0.4)
ExecuteButton.BorderColor3 = Color3.new(0, 0, 0)
ExecuteButton.Position = UDim2.new(0.75, 10, 0, 40)
ExecuteButton.Size = UDim2.new(0.25, -20, 0, 30)
ExecuteButton.Font = Enum.Font.SourceSansBold
ExecuteButton.Text = "Ejecutar"
ExecuteButton.TextColor3 = Color3.new(1, 1, 1)
